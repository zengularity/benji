<FindBugsFilter>
  <Match>
    <Class name="~com\.zengularity\.vfs\.(VFSBucketRef|VFSObjectRef)" />
    <Field name="toString" />
    <Bug pattern="IS2_INCONSISTENT_SYNC" />
  </Match>

  <Match>
    <Class name="~com\.zengularity\.vfs\.VFSTransport.*temporary.*" />
    <Method name="apply" />
    <Bug pattern="RV_RETURN_VALUE_IGNORED_BAD_PRACTICE" />
  </Match>
</FindBugsFilter>
